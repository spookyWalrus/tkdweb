{
  "name": "tkdweb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "i18nexus pull && NEXT_SKIP_TURBOPACK=false next dev",
    "build": "i18nexus pull && next build",
    "lint": "next lint",
    "cy:open": "cypress open",
    "lint:css": "stylelint '**/*.css'",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "start": "next start"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.0",
    "@hcaptcha/react-hcaptcha": "^1.12.0",
    "axios": "^1.7.7",
    "bulma": "^1.0.3",
    "clsx": "^2.1.1",
    "cypress": "^12.17.4",
    "dotenv": "^16.4.5",
    "next": "^13.5.9",
    "next-intl": "^3.26.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.83.4",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.20.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "13.5.7",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-cypress": "^3.0.0",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^8.0.3",
    "i18nexus-cli": "^3.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.49",
    "postcss-import": "^15.1.0",
    "postcss-scss": "4.0",
    "prettier": "^3.5.1",
    "sass": "^1.83.1",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "34.0",
    "stylelint-config-standard-scss": "11.0",
    "stylelint-scss": "^5.0.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "turbo": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,scss}": [
      "prettier --write",
      "stylelint --fix"
    ]
  }
}